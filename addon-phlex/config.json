{
    "name": "Phlex",
    "version": "dev",
    "slug": "phlex",
    "description": "Voice control for plex with Google assistant - repackaged for Hass.io",
    "webui": "[PROTO:ssl]://[HOST]:[PORT:5001]/Phlex/index.php",
    "startup": "system",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "i386"
    ],
    "boot": "auto",
    "host_network": true,
    "hassio_api": true,
    "privledged": [
        "NET_ADMIN"
    ],
    "ports": {
        "80/tcp":5123,
        "9000/tcp":9123
    },
    "map": [
        "ssl:rw"
    ],
    "options": {
        "port": 5001,
        "fastcgiport": 9011,
        "ssl": true,
        "certfile": "fullchain.pem",
        "keyfile": "privkey.pem"
    },
    "schema": {
        "port": "port",
        "fastcgiport": "port",
        "ssl": "bool",
        "certfile": "str",
        "keyfile": "str"
    }
}
